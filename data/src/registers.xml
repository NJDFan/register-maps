<?xml version="1.0" encoding="iso-8859-1"?>
<component	name="DIO" width="16"
			xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
			xsi:noNamespaceSchemaLocation="../schema/hti_component.xsd">
			
    <desc>
	All functional registers for the V250
	</desc>
	
    <register name="RDATA" offset="0" readOnly ="true" >
        <desc> channels 0...15 realtime input </desc>  
    </register>
    
    <register name="RDATB" offset="1" readOnly ="true" reset="0x100">
        <desc> channels 16...31 realtime input </desc>  
    </register>
    
    <register name="RDATC" offset="2" readOnly ="true" >
        <desc> channels 32...47 realtime input </desc>  
    </register>
    
    <register name="RDATD" offset="3" readOnly ="true" >
        <desc> channels 48...63 realtime input </desc>  
    </register>
    
    <register name="DDATA" offset="4" readOnly ="true" >
        <desc> channels 0...15 debounced input </desc>  
    </register>
    
    <register name="DDATB" offset="5" readOnly ="true" >
        <desc> channels 16...31 debounced input </desc>  
    </register>
    
    <register name="DDATC" offset="6" readOnly ="true" >
        <desc> channels 32...47 debounced input </desc>  
    </register>
    
    <register name="DDATD" offset="7" readOnly ="true" >
        <desc> channels 48...63 debounced input </desc>  
    </register>
    
    <register name="KDATA" offset="8">
        <desc> channels 0...15 output drive </desc>  
    </register>
    
    <register name="KDATB" offset="9">
        <desc> channels 16...31 output drive </desc>  
    </register>
    
    <register name="KDATC" offset="10">
        <desc> channels 32...47 output drive </desc>  
    </register>
    
    <register name="KDATD" offset="11">
        <desc> channels 48...63 output drive </desc>  
    </register>

</component>
